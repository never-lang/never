
use EN

enum E 
{
    ZERO,
    ONE,
    TWO = 2,
    THREE = E::TWO + 1
}

enum F
{
    ZERO,
    ONE { i : int; },
    TWO = 2,
    THREE = E::THREE
}

func calc(f : F) -> int
{
    if let (F::ONE(i) = f)
    {
        prints("OK " + i + "\n")
    }
    else
    {
        prints("NOK\n")
    };

    0
}

func calc2(i : int) -> int
{
    print(i)
}

func main() -> int
{
    calc2(EN.E::ZERO);
    calc2(EN.E::ONE);
    calc2(EN.E::TWO);
    calc2(EN.E::THREE);

    calc2(E::ZERO);
    calc2(E::ONE);
    calc2(E::TWO);
    calc2(E::THREE);

    calc2(F::ZERO);
    calc2(F::ONE(1));
    calc2(F::TWO);
    calc2(F::THREE);

    0
}

