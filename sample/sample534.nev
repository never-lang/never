
enum E
{
    ONE       = 1 shl 0,
    TWO       = E::ONE shl 1,
    FOUR      = E::TWO shl 1,
    EIGHT     = E::FOUR shl 1,
    SIXTEEN   = E::EIGHT shl 1,
    THIRTYTWO = E::SIXTEEN shl 1,
    Some { value : int; }
}

func getEight() -> E
{
    E::EIGHT
}

func main() -> int
{
    print(E::ONE);
    print(E::TWO);
    print(E::FOUR);
    print(E::EIGHT);
    print(E::SIXTEEN);
    print(E::THIRTYTWO);

    print(E::ONE or E::TWO);
    print(E::ONE or E::FOUR);
    print(-E::TWO);
    print(E::ONE shl 5);

    assert(E::ONE == 1);
    assert(E::TWO == 2);
    assert(E::FOUR == 4);
    assert(E::EIGHT == 8);
    assert(8 == E::EIGHT);
    assert(getEight() == 8);
    assert(8 == getEight());
    assert(getEight() == E::EIGHT);
    assert(E::EIGHT == getEight());
    assert(E::SIXTEEN == 16);
    assert(E::THIRTYTWO == 32);

    assert((E::ONE or E::TWO) == 3);
    assert((E::ONE or E::FOUR) == 5);
    assert(-E::TWO == -2);
    assert((E::ONE shl 5) == E::THIRTYTWO);
    assert((E::EIGHT xor E::TWO xor E::TWO) == E::EIGHT);

    0
}
