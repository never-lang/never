
enum Optional { Some{ value : int; }, None }

func getF(o : Optional) -> () -> int
{
    if let ( Optional::Some(value) = o)
    {
        let func() -> int { value }
    }
    else
    {
        let func() -> int { 1000 }
    }
}

func main() -> int
{
    let o = Optional::Some(10);
    
    print(getF(o)());
    
    0
}


