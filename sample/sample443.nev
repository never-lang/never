
func print_range( [ f1 .. t1 ] : range ) -> int
{
    assert(f1 == 10);
    assert(t1 == 30);

    0
}

func main() -> int
{
    let f = 10;
    let t = 30;
    let r1 = [ 10 .. 30 ];
    var r2 = [ f .. t ];

    print_range( r1 );

    print_range( r2 );

    r2 = r1;
    print_range( r2 );

    0
}
