
func print_cl(cl[D] : [_] : int) -> int
{
    var i = 0;
    
    for (i = 0; i < D; i = i + 1)
    {
        prints("[" + cl[i][0] + ", " + cl[i][1] + "]\n")
    };
    
    0
}

func cl(v1[D1] : int, v2[D2] : int) -> [_] : [_] : int
{
    [ [x, y] : int <- x in v1; y in v2 ] : [_] : int
}
    
func main() -> int
{
    var v1 = [1, 2, 3, 4] : int;
    var v2 = [1, 2, 3, 4, 5, 6] : int;

    print_cl(cl(v1, v2))
}

