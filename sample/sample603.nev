
func print_matrix(W[D1, D2] : float) -> int
{
    var r = 0;
    for (r = 0; r < D1; r = r + 1)
    {
        var c = 0;
        for (c = 0; c < D2; c = c + 1)
        {
            prints(W[r, c] + " ")
        };
        prints("\n")
    }
}

func T_matrix(W[D1, D2] : float) -> [_,_] : float
{
    var T = {[ D2, D1 ]} : float;
    
    var r = 0;
    for (r = 0; r < D1; r = r + 1)
    {
        var c = 0;
        for (c = 0; c < D2; c = c + 1)
        {
            T[c, r] = W[r, c]
        }
    };
    T
}

func main() -> bool 
{
    var z = 0;

    for (z = 1; z < 10; z = z  + 1 )
