        func foo(i : int, j : int) -> int { 1; 2; 3; 4 };
        20 + 30;
        let xT = T_matrix(x);
        func bar(i : int, j : int) -> int { 5; 6; 7; 8 };

        print_matrix(xT);

        0
    };

