
func print_cl(cl[D] : int) -> int
{
    var i = 0;
    
    for (i = 0; i < D; i = i + 1)
    {
        print(cl[i])
    };
    
    0
}

func cl(z : int) -> [_] : int
{
    func d(x : int) -> int
    {
        2 * x
    }

    [ y + z <- x in [10, 20, 30, 40, 50] : int; y in [d(x + 1), d(x + 2), d(x + 3)] : int ] : int
}
    
