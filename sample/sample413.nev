
enum Optional { Some { value : int; }, None }

func printOptional(o : Optional) -> bool
{
    match o
    {
        Optional::Some(value) -> assert(value == 1000);
        Optional::None -> assert(false);
    }
}

func main() -> int
{
    var o = Optional::None;
    
    o = Optional::Some(1000);
    
    printOptional(o);
    
    0
}

